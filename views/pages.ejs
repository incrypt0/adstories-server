<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <!-- Font Awesome -->
    <link
      rel="stylesheet"
      href="https://use.fontawesome.com/releases/v5.8.2/css/all.css"
    />
    <!-- Google Fonts -->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap"
    />
    <!-- Bootstrap core CSS -->
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <!-- Material Design Bootstrap -->
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/css/mdb.min.css"
      rel="stylesheet"
    />
    <!-- JQuery -->
  </head>
  <body>
    <table
      id="dtBasicExample"
      class="table table-striped table-bordered table-sm"
      cellspacing="0"
      width="100%"
    >
      <thead>
        <tr>
          <th class="th-sm">Name</th>
          <th class="th-sm">UID</th>
          <th class="th-sm">WMID</th>
          <th class="th-sm">WAnumber</th>
          <th class="th-sm">UPI ID</th>
          <th class="th-sm">Status Url</th>
          <th class="th-sm">Views Url</th>
          <th class="th-sm">Submitted</th>
          <th class="th-sm">Verified</th>
          <th class="th-sm">Payment</th>
        </tr>
      </thead>
      <tbody>
        <% results.forEach(ele => { %>
        <tr>
          <td><%= ele.name??'' %></td>
          <td><%= ele.uid ??''%></td>
          <td><%= ele.wmid??'' %></td>
          <td><%= ele.wanumber??'' %></td>
          <td><%= ele.upi??'' %></td>
          <td><a href=""><%= ele.url??'' %></a></td>
          <td><a href=""><%= ele.viewsurl ??''%></a></td>
          <td>
            <button
              class="btn btn-sm rounded decider <% if (ele.submitted) { %>btn-success <%} else { %> btn-danger<% } %>"
              val="submitted"
              dbid="<%= ele._id %>"
            >
              <%= ele.submitted??'' %>
            </button>
          </td>
          <td>
            <button
              class="btn btn-sm rounded decider  <% if (ele.verified) { %>btn-success <%} else { %> btn-danger<% } %>"
              val="submitted"
              dbid="<%= ele._id %>"
              val="verified"
              dbid="<%= ele._id %>"
            >
              <%= ele.verified??'' %>
            </button>
          </td>
          <td>
            <button
              class="btn btn-sm rounded decider  <% if (ele.payment) { %>btn-success <%} else { %> btn-danger<% } %>"
              val="submitted"
              dbid="<%= ele._id %>"
              val="payment"
              dbid="<%= ele._id %>"
            >
              <%= ele.payment??'' %>
            </button>
          </td>
        </tr>
        <% }) %>
      </tbody>

      <tfoot>
        <tr>
          <th class="th-sm">Name</th>
          <th class="th-sm">UID</th>
          <th class="th-sm">WMID</th>
          <th class="th-sm">WAnumber</th>
          <th class="th-sm">UPI ID</th>
          <th class="th-sm">Status Url</th>
          <th class="th-sm">Views Url</th>
          <th class="th-sm">Submitted</th>
          <th class="th-sm">Verified</th>
          <th class="th-sm">Payment</th>
        </tr>
      </tfoot>
    </table>
    <script type="text/javascript" src="/decider.js"></script>
  </body>
</html>
